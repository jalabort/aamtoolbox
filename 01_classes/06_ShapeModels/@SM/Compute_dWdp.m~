function [dWdp_p,dWdq,dWdb_q] = Compute_dWdp_p(obj,dWduvi)
  %Compute_dWdp Summary of this function goes here
  %   Detailed explanation goes here
  
  [duidq,dvidq] = obj.Shape2UV(obj.Compute_duvidq());
  [duidb_q,dvidb_q] = obj.Shape2UV(obj.Compute_duvidb_q());
  
  dWdq = [dWduvi * duidq;dWduvi * dvidq]; 
  dWdb = [dWduvi * duidb_q;dWduvi * dvidb_];
  
  dWdp = [dWdq,dWdb];
  
end

